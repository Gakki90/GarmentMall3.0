/**app.wxss**/

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.custom{
  position: fixed;
  width: 100%;
  top: 0;
  left: 0;
  height:170rpx;
  background: rgb(254, 238, 54);
  z-index: 500;
}

.custom text{
  color: #fff;
  font-size: 34rpx;
  font-weight: 500;
}

.custom_view
{
  display: flex;
  flex-direction: column;
  position: relative;
}

.custom_bg
{
  display: flex;
  flex-direction: column;
  margin-top: 170rpx;
  position: fixed;
}

.custom_content
{
  display: flex;
  flex-direction: column;
  margin-top: 170rpx;
  position: absolute;
  width:100%;
}
